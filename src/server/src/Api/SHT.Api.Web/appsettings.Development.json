{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
    },
    "WriteTo": {
      "ConsoleLogger": {
        "Name": "Console",
        "Theme": "Code",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      "DebugLogger": {
        "Name": "Debug",
        "Args": {
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    }
  }
}